    "@cloudflare/workers-types": "^4.20250214.0",
    "@tsndr/cloudflare-worker-jwt": "^3.1.3",
    "typescript": "^5.5.2",
    "vitest": "~2.1.9",
    "wrangler": "^3.109.2"
  },
  "dependencies": {
    "itty-router": "^5.0.18"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
