{
  "name": "artoo-api",
  "description": "Cloudflare Workers API for Artoo file management system",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "deploy": "wrangler deploy",
    "dev": "wrangler dev",
    "start": "wrangler dev",
    "remote": "wrangler dev --remote",
